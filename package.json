{
  "name": "gatewayapi",
  "version": "1.0.0",
  "author": "Samuel Kopp (https://samuelkopp.de)",
  "license": "MIT",
  "description": "JavaScript SDK for gatewayapi.",
  "funding": "https://github.com/sponsors/boywithkeyboard",
  "repository": "github:boywithkeyboard/gatewayapi",
  "keywords": [
    "gatewayapi",
    "messaging",
    "sdk",
    "sms",
    "short codes",
    "virtual numbers",
    "automation"
  ],
  "main": "./build/main.cjs",
  "module": "./build/main.mjs",
  "types": "./build/main.d.ts",
  "files": [
    "./build"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./build/main.mjs",
        "types": "./build/main.d.ts"
      },
      "require": {
        "default": "./build/main.cjs",
        "types": "./build/main.d.ts"
      }
    }
  },
  "scripts": {
    "build:cjs": "esbuild main.ts --bundle --minify --format=cjs --target=es2022 --outfile=build/main.cjs",
    "build:esm": "esbuild main.ts --bundle --minify --format=esm --target=es2022 --outfile=build/main.mjs",
    "build": "tsc && npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@drgn/std": "^0.217.0",
    "@types/node": "^20.11.20",
    "esbuild": "^0.20.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20"
  }
}
